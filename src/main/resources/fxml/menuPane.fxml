<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.filip.mp3player.controler.MenuPaneControler">
    <MenuBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

            <Menu mnemonicParsing="false" text="File">

                    <MenuItem fx:id="fileMenuItem" mnemonicParsing="false" text="Open Mp3 player" />
                    <MenuItem fx:id="dirMenuItem" mnemonicParsing="false" text="Open directory" />
                    <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" text="Close" />

            </Menu>
            <Menu fx:id="fileMenuItem1" mnemonicParsing="false" text="Help">

                    <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" text="About" />

            </Menu>

    </MenuBar>

</AnchorPane>
